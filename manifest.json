{
    "manifest_version": 3,
    "name": "Google 珍珠奶茶背景",
    "version": "1.0",
    "description": "將 Google 頁面背景更改為珍珠奶茶排列成滿版",
      "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      },
    "action": {
      "default_icon":{
        "16": "images/icon16-2.png",
        "48": "images/icon48-2.png",
        "128": "images/icon128-2.png"
      }
    },
    "content_scripts": [{
        "matches": ["https://www.google.com/*"],
        "css": ["main.css"]
      }
    ]
  }
  

